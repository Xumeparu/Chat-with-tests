{
  "name": "html",
  "version": "1.0.0",
  "description": "ВКИ НГУ. Предмет \"Проектирование и разработка интерфейсов пользователя\", Д.А.Вайнер. Чат с тестами",
  "main": "src/App.js",
  "scripts": {
    "test": "jest",
    "start": "node server.js",
    "dev": "cross-env NODE_ENV=development webpack-dev-server --config ./configs/webpack.development.js",
    "build": "cross-env NODE_ENV=production webpack --config ./configs/webpack.production.js",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "prettier": "prettier --write \"src/**/*.js\"",
    "storybook": "start-storybook -p 6006 --no-dll",
    "build-storybook": "build-storybook --no-dll",
    "loki:test": "npm run build-storybook && npx loki test --reactUri file:./storybook-static",
    "loki:update": "npm run build-storybook && npx loki update --reactUri file:./storybook-static",
    "loki:approve": "npx loki approve",
    "nightwatch": "nightwatch"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-react": "^7.10.4",
    "@storybook/addon-actions": "^6.0.28",
    "@storybook/addon-essentials": "^6.0.28",
    "@storybook/addon-links": "^6.0.28",
    "@storybook/react": "^6.0.28",
    "axios": "^0.21.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.5.2",
    "babel-loader": "^8.1.0",
    "chromedriver": "^87.0.0",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^6.2.1",
    "core-js": "^3.7.0",
    "cross-env": "^7.0.2",
    "css-loader": "^5.0.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.5",
    "eslint": "^7.13.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-prettier": "^6.14.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-import": "^2.21.2",
    "eslint-plugin-jest": "^24.1.0",
    "eslint-plugin-jsx-a11y": "^6.3.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.0.0",
    "eslint-plugin-sonarjs": "^0.5.0",
    "eslint-plugin-standard": "^4.0.2",
    "file-loader": "^6.1.1",
    "formik": "^2.2.3",
    "history": "^5.0.0",
    "html-webpack-plugin": "^4.5.0",
    "husky": "^4.3.0",
    "jest": "^26.5.3",
    "lint-staged": "^10.5.0",
    "loki": "^0.25.1",
    "nightwatch": "^1.5.1",
    "prettier": "^2.1.2",
    "react-is": "^17.0.1",
    "react-router-dom": "^5.2.0",
    "react-test-renderer": "^16.14.0",
    "sass": "^1.27.0",
    "sass-loader": "^10.0.3",
    "style-loader": "^2.0.0",
    "webpack": "^4.44.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^5.2.0"
  },
  "jest": {
    "setupFiles": [
      "./setupTests.js"
    ],
    "transform": {
      "^.+\\.js$": "babel-jest",
      ".+\\.(css|styl|less|sass|scss)$": "jest-transform-css"
    }
  },
  "lint-staged": {
    "*.(js|jsx)": [
      "eslint --fix"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "npx lint-staged"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Xumeparu/Chat-with-tests.git"
  },
  "bugs": {
    "url": "https://github.com/Xumeparu/Chat-with-tests/issues"
  },
  "homepage": "https://github.com/Xumeparu/Chat-with-tests#readme",
  "loki": {
    "configurations": {
      "chrome.laptop": {
        "target": "chrome.app",
        "width": 1366,
        "height": 768,
        "deviceScaleFactor": 1,
        "mobile": false
      },
      "chrome.iphone7": {
        "target": "chrome.app",
        "preset": "iPhone 7"
      }
    }
  }
}
